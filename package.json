{
  "name": "shebang-snippets",
  "displayName": "Shebang Snippets",
  "description": "Snippets for shebang lines for different types of scripts.",
  "version": "1.0.0",
  "publisher": "rpinski",
  "author": "Andreas Weizel",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rpinski/vscode-shebang-snippets"
  },
  "icon": "images/shebang-logo.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Snippet",
    "Script",
    "Shebang",
    "Comment",
    "Shell",
    "Bash",
    "Lua",
    "Ruby",
    "Python",
    "F#",
    "FSharp",
    "Perl",
    "JS",
    "JavaScript"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "pnpm package",
    "compile": "webpack --mode development",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "build:vsix": "vsce package --no-dependencies",
    "publish:vscode": "vsce publish --no-dependencies",
    "publish:openvsx": "ovsx publish --no-dependencies",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm compile-tests && pnpm compile && pnpm lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "20.19.25",
    "@types/vscode": "1.86.0",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "@vscode/vsce": "3.7.0",
    "ovsx": "0.10.6",
    "eslint": "9.39.1",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "webpack": "5.103.0",
    "webpack-cli": "6.0.1"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
